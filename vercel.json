{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "bash vercel-build.sh",
  "outputDirectory": ".",
  "framework": "flask"
}
